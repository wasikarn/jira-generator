    dateFormat HH:mm
    axisFormat %H:%M
    tickInterval 10minute

    section Normal Schedule
    P2 Campaign Ad A 15s         :done, n1, 07:50, 3m
    P4 House Ad 10s              :done, n2, 07:53, 2m
    P2 Campaign Ad B 15s         :done, n3, 07:55, 3m
    P2 Campaign Ad C 15s         :done, n4, 07:58, 2m

    section P1-TK Takeover Block
    TK START                     :vert, ms1, 08:00, 1m
    Brand X Creative loop x120   :crit, tk, 08:00, 60m
    TK END                       :vert, ms2, 09:00, 1m

    section Resume and Make-Good
    Make-good Ad A interrupted   :active, mg1, 09:00, 3m
    P2 Campaign Ad D 15s         :r1, 09:03, 3m
    P4 House Ad 10s              :r2, 09:06, 2m
    P2 Campaign Ad E 15s         :r3, 09:08, 2m
